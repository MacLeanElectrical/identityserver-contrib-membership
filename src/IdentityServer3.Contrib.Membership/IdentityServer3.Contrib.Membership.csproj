<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Copyright>Copyright © 2017 onwards</Copyright>
    <TargetFrameworks>net452;netstandard2.0</TargetFrameworks>
    <Configurations>Debug;Release</Configurations>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/sma73648/identityserver-contrib-membership</PackageProjectUrl>
    <PackageLicenseUrl>https://www.mozilla.org/en-US/MPL/2.0/</PackageLicenseUrl>
    <PackageIconUrl>https://identityserver.github.io/Documentation/assets/images/icons/IDserver_icon128.jpg</PackageIconUrl>
    <RepositoryUrl>https://github.com/sma73648/identityserver-contrib-membership</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Stuart Banks, Scott Mackay @wwwlicious</Authors>
    <Description>An IdentityServer plugin for authenticating against a ASP.NET 2.0 Membership Database</Description>
    <PackageTags>IdentityServer</PackageTags>
    <PackageReleaseNotes>See project url for details</PackageReleaseNotes>
    <Version>$(Version)</Version>
    <PackageOutputPath>$(PackageOutputPath)</PackageOutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="GitLink" Version="3.1.0" />
    <PackageReference Include="IdentityModel" Version="2.8.1" />
    <PackageReference Include="IdentityServer3" Version="2.*" />
    <PackageReference Include="System.Data.SqlClient" Version="4.5.1" />
  </ItemGroup>
</Project>