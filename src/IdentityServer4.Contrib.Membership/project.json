{
  "version": "4.0.0-*",
  "authors": [ "Stuart Banks" ],
  "description": "An IdentityServer plugin for authenticating against a ASP.NET 2.0 Membership Database",

  "packOptions": {
    "tags": [ "IdentityServer" ],
    "projectUrl": "https://github.com/MacLeanElectrical/identityserver-contrib-membership",
